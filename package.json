{
  "name": "kaia-wallet-module",
  "version": "1.0.0",
  "description": "Kaia 블록체인 지갑 모듈",
  "keywords": [
    "kaia",
    "blockchain",
    "wallet",
    "typescript"
  ],
  "repository": "git@kyle-park-io:kyle-park-io/kyle-kaia.git",
  "license": "MIT",
  "author": "kyle-park-io <andy3638@naver.com>",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "pnpm run build",
    "sort-package": "sort-package-json",
    "test": "jest --selectProjects unit",
    "test:all": "jest",
    "test:integration": "jest --selectProjects integration",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:unit": "jest --selectProjects unit",
    "test:watch": "jest --selectProjects unit --watch"
  },
  "dependencies": {
    "@kaiachain/ethers-ext": "^2.0.8",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "^3.4.2",
    "sort-package-json": "^3.4.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.40.0",
    "undici": "^7.15.0"
  },
  "packageManager": "pnpm@10.14.0"
}
